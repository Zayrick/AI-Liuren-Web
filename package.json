{
  "name": "ai-api-proxy",
  "version": "1.0.0",
  "description": "基于 Cloudflare Pages Functions 的通用 AI API 转发代理工具，支持原样转发请求到各种AI供应商。",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "wrangler pages dev . --live-reload",
    "dev:lan": "wrangler pages dev . --live-reload --ip 0.0.0.0",
    "deploy": "wrangler pages publish . --project-name ai-api-proxy",
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zayrick/AI-API-Proxy.git"
  },
  "bugs": {
    "url": "https://github.com/Zayrick/AI-API-Proxy/issues"
  },
  "homepage": "https://github.com/Zayrick/AI-API-Proxy#readme",
  "keywords": [
    "Cloudflare",
    "Pages",
    "Workers",
    "AI",
    "API",
    "Proxy",
    "OpenAI"
  ],
  "author": "Zayrick",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^4.20.0"
  }
}
